
  <!-- sidebar-wrapper  -->
  <main class="page-content">
    <div class="container-fluid">
      <h2>Người dùng</h2>
      <hr>
      <div class="row">
        <div class="col-lg-6">
            <table id="example" class="table table-striped table-bordered" style="width:100%">
              <thead>
                  <tr>
                      <th>Tên</th>
                      <th>Tên tài khoản</th>
                      <th>Email</th>
                      <th>SĐT</th>
                  </tr>
              </thead>
              <tbody>
                <% users.forEach(u => {%>
                <tr class="user-row user-<%=u.accountId%>" onclick=getUserInfo(this) data-id="<%=u.accountId%>">
                  <td>
                    <%=u.name%>
                  </td>
                  <td>
                    <%=u.username%>
                  </td>
                  <td>
                    <%=u.email%>
                  </td>
                  <td>
                    <%=u.phone%>
                  </td>
                </tr>
                <%})%>
              </tbody>
              <tfoot>
                  <tr>
                      <th>Tên</th>
                      <th>Tên tài khoản</th>
                      <th>Email</th>
                      <th>SĐT</th>
                  </tr>
              </tfoot>
            </table>
        </div>
        <div class="col-lg-6">
          <div class="user-container">
              
          </div>
        </div>
      </div>
    </div>
  </main>
  <!-- page-content" -->