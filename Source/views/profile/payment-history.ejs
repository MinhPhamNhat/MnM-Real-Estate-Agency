
<h1>Lịch sử nạp tiền</h1>
<hr>
<div class="contact-list">
    <table>
        <tr>
            <th>
                Loại thẻ
            </th>
            <th>
                Mã thẻ
            </th>
            <th>
                Mã seri
            </th>
            <th>
                Mệnh giá
            </th>
            <th>
                Tình trạng
            </th>
            <th>
                Thời gian
            </th>
        </tr>
        <%payments.forEach(value=>{%>
            <tr>
                <td>
                    <%=value.cardType%>
                </td>
                <td>
                    <%=value.code%>
                </td>
                <td>
                    <%=value.seri%>
                </td>
                <td>
                    <%=value.denom?(value.denom+".000 VNĐ"):" VNĐ"%>
                </td>
                <td>
                    <%=value.status&&value.isAproved?"Nạp thành công":!value.isAproved&&value.status?"Đang chờ xử lý":"Nạp thất bại"%>
                </td>
                <td>
                    <%=locals.shorterFormatForDateTime(value.date)%>
                </td>
            </tr>
        <%})%>
    </table>
</div>